			<h3>Members</h3>
			<section class="api-members">
				<article>
					<h4>Abstract</h4>
					<ul>{+classes.a+}</ul>
				</article>
				<article>
					<h4>Static</h4>
					<ul>{+classes.s+}</ul>
				</article>
				<article>
					<h4>Widget</h4>
					<ul>{+classes.w+}</ul>
				</article>
			</section>
		</section>
	</section>

	
